    "(defn abundantes [n]\n",
    "    (if (> (reduce + (let [y n]\n",
    "  (for [x (range 1 y)\n",
    "       :when (zero? (rem y x))]x))) \n",
    "           n)\n",
    "        (println \"0\")\n",
    "        (println \"1\"))\n",
    "    )"
